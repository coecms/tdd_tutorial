@test
subroutine test_no_params()
    use pfunit_mod
    use greeter_mod
    implicit none
    @assertEqual("Hello World", greeter())
end subroutine test_no_params

@test
subroutine test_pluto()
    use pfunit_mod
    use greeter_mod
    implicit none
    @assertEqual("Hello Pluto", greeter(cAddressee = "Pluto"))
end subroutine test_pluto
